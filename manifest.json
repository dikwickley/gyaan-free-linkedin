{
    "manifest_version": 3,
    "name": "Gyaan Free Linkedin",
    "version": "1.0",
    "description": "Hide posts from specific users on LinkedIn",
    "permissions": ["storage", "activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["content.js"]
      }
    ]
  }